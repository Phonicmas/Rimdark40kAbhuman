<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>RimDark 40k - Mankinds Finest</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/FactionDef[defName="BEWH_ImperiumFaction"]/xenotypeSet/xenotypeChances</xpath>
                    <value>
                        <BEWH_Ratling>0.15</BEWH_Ratling>
                        <BEWH_Ogryn>0.15</BEWH_Ogryn>
                    </value>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/AbilityDef[defName="BEWH_NavigatorExposeWarpEye"]/comps/li[@Class="Abhuman40k.CompProperties_AbilityWarpEyeGaze"]/stunnedNotKilledGeneDef</xpath>
                    <value>
                        <li>BEWH_TempestusOcularium</li>
                        <li>BEWH_MnemosyneMindshield</li>
                        <li>BEWH_SigmaPariah</li>
                        <li>BEWH_UpsilonPariah</li>
                        <li>BEWH_OmegaPariah</li>
                    </value>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/AbilityDef[defName="BEWH_NavigatorExposeWarpEye"]/comps/li[@Class="Abhuman40k.CompProperties_AbilityWarpEyeGaze"]/stunnedNotKilledPawnKindDef</xpath>
                    <value>
                        <li>BEWH_CustodesPawn</li>
                        <li>BEWH_LordCommanderPawn</li>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>