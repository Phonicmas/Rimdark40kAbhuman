<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>RimDark 40k - Mankinds Finest</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/FactionDef[defName="BEWH_ImperiumFaction"]/xenotypeSet/xenotypeChances</xpath>
                    <value>
                        <BEWH_Ratling>0.15</BEWH_Ratling>
                        <BEWH_Ogryn>0.15</BEWH_Ogryn>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>