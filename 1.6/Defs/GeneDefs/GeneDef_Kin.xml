<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Kin: Cloneskein -->
    <GeneDef>
        <defName>BEWH_KinCloneskein</defName>
        <label>cloneskein</label>
        <description>???</description>
        <iconPath>UI/Genes/Placeholder</iconPath>
        <biostatCpx>1</biostatCpx>
        <biostatMet>0</biostatMet>
        <displayOrderInCategory>41</displayOrderInCategory>
        <geneClass>Core40k.Gene_AddRandomGeneAndOrTraitByWeight</geneClass>
        <customEffectDescriptions>
            <li>Can be grown from a Growth Vat connected to a Votann Core.</li>
            <li>Can be absorbed by a Votann Core upon death, giving learning bonus to all living Kin.</li>
            <li>Will have a random Cloneskein mutation.</li>
        </customEffectDescriptions>
        <exclusionTags>
            <li>Psyker</li>
            <li>Pariah</li>
        </exclusionTags>
        <biologicalAgeTickFactorFromAgeCurve>
            <points>
                <li>(0, 10)</li>
                <li>(18, 1)</li>
            </points>
        </biologicalAgeTickFactorFromAgeCurve>
        <modExtensions>
            <li Class="Core40k.DefModExtension_AddRandomGeneByWeight">
                <possibleGenesToGive>
                    <li>
                        <key>BEWH_KinCloneskeinTemplateSubdermalLayer</key>
                        <value>20</value>
                    </li>
                    <li>
                        <key>BEWH_KinCloneskeinTemplateTunnelEyes</key>
                        <value>20</value>
                    </li>
                    <li>
                        <key>BEWH_KinCloneskeinTemplateGravbornSinews</key>
                        <value>20</value>
                    </li>
                    <li>
                        <key>BEWH_KinCloneskeinTemplateStoneheart</key>
                        <value>20</value>
                    </li>
                    <li>
                        <key>BEWH_KinCloneskeinTemplateRadvein</key>
                        <value>20</value>
                    </li>
                </possibleGenesToGive>
            </li>
        </modExtensions>
    </GeneDef>

