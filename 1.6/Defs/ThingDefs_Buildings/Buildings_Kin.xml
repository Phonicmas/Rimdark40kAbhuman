<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Ancestor Core -->
  <ThingDef ParentName="BuildingBase">
    <defName>BEWH_AncestorCore</defName>
    <label>ancestore core</label>
    <description>???</description>
    <thingClass>Abhuman40k.Building_AncestorCore</thingClass>
    <containedPawnsSelectable>true</containedPawnsSelectable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <fillPercent>0.4</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>Things/Building/Ship/ShipComputerCore</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <shadowData>
        <volume>(2.76, 0.5, 2.66)</volume>
        <offset>(0,0,-0.05)</offset>
      </shadowData>
    </graphicData>
    <size>(3, 3)</size>
    <canOverlapZones>false</canOverlapZones>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <defaultPlacingRot>North</defaultPlacingRot>
    <designationCategory>BEWH_RimDarkCategory</designationCategory>
    <uiOrder>2100</uiOrder>
    <!--<researchPrerequisites>
      <li>BEWH_GeneseedCreation</li>
    </researchPrerequisites>-->
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
      <WorkToBuild>9000</WorkToBuild>
      <Mass>85</Mass>
      <Flammability>0.2</Flammability>
    </statBases>
    <costList>
      <Steel>300</Steel>
      <Plasteel>40</Plasteel>
      <Gold>5</Gold>
      <ComponentIndustrial>16</ComponentIndustrial>
      <ComponentSpacer>8</ComponentSpacer>
    </costList>
    <constructionSkillPrerequisite>10</constructionSkillPrerequisite>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
      <li>Core40k.PlaceWorker_OnlyOnePerMap</li>
    </placeWorkers>
    <building>
      <workTableRoomRole>Laboratory</workTableRoomRole>
    </building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>1500</basePowerConsumption>
      </li>
      <li Class="CompProperties_Facility">
        <maxSimultaneous>5</maxSimultaneous>
        <maxDistance>20</maxDistance>
      </li>
      <li Class="CompProperties_Flickable" />
    </comps>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>BEWH_KinCrucible</defName>
    <label>crucible</label>
    <description>???</description>
    <thingClass>Abhuman40k.Building_Crucible</thingClass>
    <containedPawnsSelectable>true</containedPawnsSelectable>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>Things/Building/Misc/GrowthVat/GrowthVat</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,2)</drawSize>
      <shadowData>
        <volume>(0.85, 0.3, 1.7)</volume>
      </shadowData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <defaultPlacingRot>North</defaultPlacingRot>
    <size>(1,2)</size>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>8000</WorkToBuild>
      <Mass>30</Mass>
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <Steel>150</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>42</pathCost>
    <blockWind>true</blockWind>
    <drawerType>MapMeshAndRealTime</drawerType>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <designationCategory>BEWH_RimDarkCategory</designationCategory>
    <uiOrder>2200</uiOrder>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <inspectorTabs>
      <li>ITab_Genes</li>
    </inspectorTabs>
    <!--<researchPrerequisites>
      <li>GrowthVats</li>
    </researchPrerequisites>-->
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <haulToContainerDuration>120</haulToContainerDuration>
      <workTableRoomRole>Laboratory</workTableRoomRole>
      <fixedStorageSettings>
        <filter>
          <categories>
            <li>Foods</li>
          </categories>
          <specialFiltersToDisallow>
            <li>AllowPlantFood</li>
          </specialFiltersToDisallow>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
          <categories>
            <li>Foods</li>
          </categories>
          <disallowedCategories>
            <li>EggsFertilized</li>
          </disallowedCategories>
          <disallowedThingDefs>
            <li>InsectJelly</li>
            <li>MealLavish</li>
            <li>MealLavish_Veg</li>
            <li>MealLavish_Meat</li>
            <li>HemogenPack</li>
            <li>Chocolate</li>
          </disallowedThingDefs>
        </filter>
      </defaultStorageSettings>
    </building>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>200</basePowerConsumption>
        <idlePowerDraw>80</idlePowerDraw>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>BEWH_AncestorCore</li>
        </linkableFacilities>
      </li>
    </comps>
  </ThingDef>

</Defs>